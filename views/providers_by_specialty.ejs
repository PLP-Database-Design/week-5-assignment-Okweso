<table>
    <thead>
        <tr>
            <th>Provider ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Provider Specialty</th>
            <th>Email Address</th>
            <th>Phone Number</th>
            <th>Date Joined</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => { %>
            <tr>
                <td><%= row.provider_id %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
                <td><%= row.provider_specialty %></td>
                <td><%= row.email_address %></td>
                <td><%= row.phone_number %></td>
                <td><%= row.date_joined %></td>
            </tr>
        <% }); %>
    </tbody>
  </table>
  