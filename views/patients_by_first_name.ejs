<table>
    <thead>
        <tr>
            
            <th>First Name</th>
            <th>Last Name</th>
            <th>Provider Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => { %>
            <tr>
                <td><%= row.patient_id %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
                <td><%= row.date_of_birth %></td>
                <td><%= row.gender %></td>
                <td><%= row.language %></td>
            </tr>
        <% }); %>
    </tbody>
  </table>
  